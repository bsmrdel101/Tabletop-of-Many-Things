{
  "name": "virtual-table",
  "version": "1.0.0",
  "description": "A virtual tabletop fop 5e D&D.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint:js": "eslint ./ --ignore-path .gitignore",
    "lint:css": "stylelint '**/*.scss'",
    "lint": "run-s lint:js lint:css",
    "server": "nodemon server/server.js",
    "sass": "sass --watch src/styles/main.scss:src/buildjs/main.css",
    "tsc": "esbuild src/*.ts --format=esm --bundle=true --splitting=true --outdir=src/buildjs --watch --sourcemap=inline",
    "docker:setup": "rm -rf ./.postgres-data && docker-compose up",
    "dev": "run-p server sass tsc docker:setup"
  },
  "engines": {
    "node": "16.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsmrdel101/Virtual-Table.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bsmrdel101/Virtual-Table/issues"
  },
  "homepage": "https://github.com/bsmrdel101/Virtual-Table#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "sass": "^1.56.1",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.2",
    "uuid": "^8.3.2"
  }
}
